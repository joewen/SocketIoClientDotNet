<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>0.0.4</Version>
    <PackageId>SocketIoClientDotNet.Standard</PackageId>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>Socket.IO Client Library for .NET Standard</Description>
    <PackageLicenseFile>LICENSE.md</PackageLicenseFile>
    <RepositoryUrl>https://github.com/joewen/EngineIoClientDotNet</RepositoryUrl>
    <Authors>Joe Wen</Authors>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="EngineIoClientDotNet.Standard" Version="0.0.4" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\LICENSE.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
